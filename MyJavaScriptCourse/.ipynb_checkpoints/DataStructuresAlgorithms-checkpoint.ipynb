   "source": [
    "function factorial(n) {\n",
    "  if (n == 0) {\n",
    "    return 1;\n",
    "  } else {\n",
    "    return factorial(n - 1) * n;\n",
    "  }\n",
    "}\n",
    "console.log(factorial(10)); // 3628800"
   ]
